{
    "name": "getsky/phalcon-skeleton-provider",
    "type": "library",
    "description": "Providers for skeleton",
    "keywords": [ "framework", "phalcon", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "JimmDiGrizli",
            "email": "getmansk-y@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": ">=1.2.4",

        "getsky/phalcon-autoload-services": "1.1.*",
        "getsky/phalcon-config-loader": ">1.0.0",
        "getsky/phalcon-bootstrap": ">1.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "squizlabs/php_codesniffer": "2.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\Phalcon\\Provider\\": "src/"
        }
    }
